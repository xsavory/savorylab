    "build:react-vite-basic": "turbo run build --filter=react-vite-basic...",
    "preview:react-vite-basic": "turbo run preview --filter=react-vite-basic",
    "dev:react-vite-rq": "turbo run dev --filter=react-vite-rq...",
    "build:react-vite-rq": "turbo run build --filter=react-vite-rq...",
    "preview:react-vite-rq": "turbo run preview --filter=react-vite-rq"
  },
  "devDependencies": {
    "@turbo/gen": "^2.5.6",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
