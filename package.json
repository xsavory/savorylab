{
  "name": "savorylab",
  "private": true,
  "scripts": {
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:test-app": "turbo run dev --filter=test-app...",
    "build:test-app": "turbo run build --filter=test-app...",
    "preview:test-app": "turbo run preview --filter=test-app",
    "dev:test-app-v2": "turbo run dev --filter=test-app-v2...",
    "build:test-app-v2": "turbo run build --filter=test-app-v2...",
    "preview:test-app-v2": "turbo run preview --filter=test-app-v2",
    "dev:test-app-v3": "turbo run dev --filter=test-app-v3...",
    "build:test-app-v3": "turbo run build --filter=test-app-v3...",
    "preview:test-app-v3": "turbo run preview --filter=test-app-v3",
    "dev:test-app-v4": "turbo run dev --filter=test-app-v4...",
    "build:test-app-v4": "turbo run build --filter=test-app-v4...",
    "preview:test-app-v4": "turbo run preview --filter=test-app-v4",
    "dev:test-app-v5": "turbo run dev --filter=test-app-v5...",
    "build:test-app-v5": "turbo run build --filter=test-app-v5...",
    "preview:test-app-v5": "turbo run preview --filter=test-app-v5",
